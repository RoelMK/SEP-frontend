{
  "name": "diabetes-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "echarts": "^5.1.1",
    "i18n": "^0.13.3",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.1",
    "v-calendar": "^2.3.0",
    "vue": "^2.6.11",
    "vue-cookies": "^1.7.4",
    "vue-echarts": "*",
    "vue-feedback-reaction": "^1.2.0",
    "vue-i18n": "^8.24.4",
    "vue-router": "^3.5.1",
    "vue-slider-component": "^3.2.11",
    "vue-toasted": "^1.1.28",
    "vuetify": "^2.4.11",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/composition-api": "*",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "moment-range": "^4.0.2",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "vue/html-indent": [
        "error",
        4
      ],
      "vue/max-len": [
        "error",
        {
          "code": 80,
          "template": 150,
          "comments": 150,
          "ignoreComments": false,
          "ignoreTrailingComments": false,
          "ignoreUrls": true,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false,
          "ignoreHTMLAttributeValues": false,
          "ignoreHTMLTextContents": false
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": false,
          "ignoreComments": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
